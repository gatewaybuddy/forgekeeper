# Example environment variables for Forgekeeper
# Copy this file to .env and adjust values as needed.

# URL of the language model API used by the backend
LLM_API_URL=http://localhost:1234/v1/completions
LLM_MODE=chat
LLM_PROMPT_STYLE=chatml
LLM_MODEL_NAME=internlm

# Address of the MQTT broker used for message routing
MQTT_BROKER=localhost
