{
  "name": "@forgekeeper/v2",
  "version": "2.0.0",
  "description": "Forgekeeper v2 - Hybrid Multi-Model Consciousness Architecture",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio"
  },
  "keywords": [
    "ai",
    "consciousness",
    "multi-agent",
    "gwt",
    "hybrid-inference"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "@graphql-tools/schema": "^10.0.3",
    "@prisma/client": "^5.20.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.16.0",
    "pino": "^8.19.0",
    "pino-pretty": "^11.0.0",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.19",
    "@types/ws": "^8.5.10",
    "prisma": "^5.20.0",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5",
    "vitest": "^1.3.1"
  },
  "engines": {
    "node": ">=22.0.0"
  }
